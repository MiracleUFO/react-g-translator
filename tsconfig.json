{
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
  },
  "include": [
    "*.eslintrc.js",
    "*.ts",
    "*.tsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.test.ts",
    "**/*.test.tsx",
  ],
}